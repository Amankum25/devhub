{
  "name": "devhub-ai",
  "version": "1.0.0",
  "description": "AI-Powered Developer Hub Platform",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/app.js",
    "client:dev": "vite",
    "build": "npm run client:build && npm run server:build",
    "client:build": "vite build",
    "server:build": "node server/scripts/build.js",
    "start": "NODE_ENV=production node server/app.js",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "db:migrate": "node server/scripts/migrate.js",
    "db:seed": "node server/scripts/seed.js",
    "db:reset": "node server/scripts/reset.js",
    "setup": "npm install && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "devhub",
    "ai",
    "developer",
    "platform",
    "react",
    "express",
    "sqlite"
  ],
  "author": "DevHub Team",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/devhub-ai.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/devhub-ai/issues"
  },
  "homepage": "https://devhub-ai.com"
}
